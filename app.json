{
  "expo": {
    "name": "Plinko Infinity",
    "slug": "plinko-infinity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icons/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#14102a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sevens.plinko.infinity",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.sevens.plinko.infinity",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./icons/icon.png",
        "backgroundColor": "#14102a"
      }
    },
    "web": {
      "favicon": "./icons/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a916a738-e3a1-49ad-9c21-f8bb6feb7bf6"
      }
    },
    "owner": "jerrythemouse"
  }
}
